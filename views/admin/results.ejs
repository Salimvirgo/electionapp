            <div class="row">
                <div class="col">
                    <div class="card shadow mb-3">
                        <div class="card-header py-3">
                            <p class="text-primary m-0 font-weight-bold">Presidential Election Result Graph</p>
                        </div>
                        <div class="card-body">
                            <div class="table-responsive">
                                <table class="table my-0" id="dataTable">
                                    <thead>
                                        <tr>
                                            <th>Voter Name</th>
                                            <th>Candidate Name</th>
                                            
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <% if (typeof allVotes !='undefined' ) { %>
                                            <% allVotes.forEach(voter=> { %>
                                                <tr>
                                                    <td>
                                                        <%= votes.voters_id %>
                                                    </td>
                                                    <td>
                                                        <%= votes.department_name %>
                                                    </td>
                                                </tr>
    
                                                <% }) %>
                                                    <% } %>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
